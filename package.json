{
  "name": "kcdmod",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder"
  },
  "keywords": [],
  "author": "itsbttq",
  "license": "ISC",
  "description": "Launcher for KCDMOD",
  "dependencies": {
    "axios": "^1.8.2",
    "create-desktop-shortcuts": "^1.11.0",
    "extract-zip": "^2.0.1"
  },
  "devDependencies": {
    "electron": "^35.0.0",
    "electron-builder": "^25.1.8",
    "patch-package": "^8.0.0"
  },
  "build": {
    "asar": true,
    "asarUnpack": [
      "node_modules/create-desktop-shortcuts/src/windows.vbs"
    ],
    "removePackageScripts": true,
    "files": [
      "**/*",
      "!**/*.map"
    ],
    "win": {
      "icon": "KCDMOD.ico",
      "target": [
        "portable"
      ]
    }
  }
}
